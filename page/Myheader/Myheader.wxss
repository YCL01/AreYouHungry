.head{
  width: 100%;
  height: 250rpx;
  background: rgba(0, 0, 0, 0.6);
  color: white;
}
.usehead{
  height: 250rpx;
  display: flex;
  justify-content: space-around;
  align-items:center;
}
.headid{
  width: 150rpx;
  height: 150rpx;
  background: white;
  border-radius:16rpx; 
}
.headid>image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 16rpx;
}
.manjian{
  display: flex;
  line-height: 50rpx;
}
.manjian image{
  height: 50rpx;
  width: 50rpx;
  
}
.namhed{
  display: flex;
  line-height: 50rpx;
  margin-top:10rpx; 
}
.namhed image{
  height: 50rpx;
  width: 80rpx;
}
.tiao{
  width: 100%;
  height: 95rpx;
  border-bottom: 1rpx solid #cccccc;
  display: flex;
  justify-content: space-around;
}
.tiao navigator{
  line-height: 95rpx;
}